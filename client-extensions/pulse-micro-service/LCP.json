{
  "kind": "Deployment",
  "id": "pulse",
  "deploy": true,
  "memory": 512,
  "cpu": 1,
  "loadBalancer": {
    "targetPort": 80
  },
  "ports": [
    {
      "port": 80,
      "external": false
    }
  ],
  "readinessProbe": {
    "httpGet": {
      "path": "/ready",
      "port": 80
    },
    "initialDelaySeconds": 120,
    "periodSeconds": 10,
    "timeoutSeconds": 4,
    "failureThreshold": 3,
    "successThreshold": 1
  },
  "livenessProbe": {
    "httpGet": {
      "path": "/ready",
      "port": 80
    },
    "initialDelaySeconds": 100,
    "periodSeconds": 10,
    "timeoutSeconds": 4,
    "failureThreshold": 3,
    "successThreshold": 1
  },
  "emv": {
    "PULSE_CLIENT_ID": "",
    "PULSE_CLIENT_SECRET": ""
  }
}